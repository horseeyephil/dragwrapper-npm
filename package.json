{
  "name": "dragwrapper",
  "version": "1.1.1",
  "description": "A lightweight higher-order function that can take any React class component and make it draggable.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/horseeyephil/dragwrapper-npm"
  },
  "scripts": {
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production"
  },
  "keywords": [
